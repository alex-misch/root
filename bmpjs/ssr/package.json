{
  "name": "serverside-render",
  "version": "0.0.1",
  "description": "run ECMA SCRIPT Modules on server",
  "main": "index.mjs",
  "scripts": {
    "render": "node --experimental-modules --no-warnings --experimental-vm-modules render.mjs --url=/ --src=https://d35qz7xn7nfid5.cloudfront.net/ssr/index.es.js",
    "_render": "node --experimental-modules --no-warnings --experimental-vm-modules render.mjs --url=/ --src=https://d35qz7xn7nfid5.cloudfront.net/ssr/index.es.js",
    "sitemap": "node --experimental-modules --no-warnings --experimental-vm-modules sitemap.mjs --output=stdout --src=https://d35qz7xn7nfid5.cloudfront.net/ssr/index.es.js",
    "dockerstart": "docker-compose exec ssrbmp sh -c 'npm run render'",
    "test": "node --experimental-modules --no-warnings ./node_modules/.bin/jest"
  },
  "author": "boomfunc",
  "license": "ISC",
  "bmp": {
    "-external": {
      "bmp-core": "https://jetsmarter.com/data/website/code/test/bmp-core.js",
      "bmp-router": "https://jetsmarter.com/data/website/code/test/bmp-router.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "verbose": true,
    "bail": false,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "mjs"
    ],
    "modulePathIgnorePatterns": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "node-fetch-polyfill": "^2.0.6",
    "elementtree": "^0.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.0.0",
    "jest": "^24.1.0"
  }
}
