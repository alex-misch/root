{
	"name": "playground",
	"version": "1.0.0",
	"description": "nope",
	"main": "app.js",
	"scripts": {
		"test": "jest",
		"dockerbuild": "docker run -it --rm -v $(pwd):/src -w /src node:10-alpine node --experimental-modules ./build.mjs",
		"bmpinstall": "docker-compose exec component sh -c 'cd project && node --experimental-modules ./install.mjs --no-cache'",
		"build": "node --experimental-modules ./build.mjs",
		"start": "sh ./bmp_modules/bmpjs/bundler/launch.sh ./project"
	},
	"author": "",
	"license": "ISC",
	"bmp": {
		"dependencies": {
			"bmpjs/bundler": "latest",
			"bmpjs/bmp-core": "latest",
			"bmpjs/bmp-router": "latest"
		}
	},
	"dependencies": {
		"@babel/core": "^7.1.2",
		"@babel/plugin-transform-modules-amd": "^7.1.0",
		"@babel/preset-env": "^7.1.0",
		"babel-plugin-syntax-object-rest-spread": "^6.13.0",
		"fs-extra": "^7.0.1",
		"rollup": "^0.66.6",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-commonjs": "^9.2.0"
	}
}
